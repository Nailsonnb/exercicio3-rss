<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="GERAL">
        <EditTextPreference
            android:defaultValue="@string/rssfeed"
            android:key="rssfeedlink"
            android:singleLine="true"
            android:title="Link do Feed"
            android:dialogTitle="Troque o link do feed"
            android:dialogMessage="Digite o link"
            android:positiveButtonText="Salvar"
            android:negativeButtonText="Cancelar" />
        <ListPreference
            android:title="Tempo de Atualização do feed"
            android:key="@string/timeset"
            android:defaultValue="@string/dois"
            android:entries="@array/SetTime"
            android:entryValues="@array/SetTimeValues" />
    </PreferenceCategory>
</PreferenceScreen>